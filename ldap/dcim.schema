objectclass ( 1.3.6.1.4.1.53407.2.4.1 NAME 'dcimIManagedElement'
	DESC 'An abstract class that provides a common superclass for various managed elements'
	SUP top
	ABSTRACT
	MAY ( dcimCaption $ dcimDescription ) )
objectclass ( 1.3.6.1.4.1.53407.2.4.2 NAME 'dcimIManagedSystemElement'
	DESC 'Base class for the System Element hierarchy.
 Examples: software components, such as files; and devices, such as disk
 drives and controllers, and physical components such as chips and cards'
	SUP dcimManagedElement
	ABSTRACT
	MAY ( dcimInstallDate $ dcimName $ dcimStatus ) )
objectclass ( 1.3.6.1.4.1.53407.2.4.3 NAME 'dcimIPhysicalElement'
	DESC ''
	SUP dcimIManagedSystemElement
	ABSTRACT
	MAY ( dcimCreationClassName $ dcimManufactureDate $ dcimManufacturer $ dcimModel $ dcimPartNumber $ dcimOtherIdentifyingInfo $ dcimSKU $ dcimSerialNumber $ dcimVersion $ dcimTag ) )
